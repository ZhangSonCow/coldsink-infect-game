{
  "format_version": "1.8.0",
  "animations": {
    "animation.machine_hero_energy.func": {
      "loop": true,
      "animation_length": 0.3,
      "timeline": {
        "0.0": [
          "/scoreboard players add @s[scores={bullet_hero=..119}] bullet_hero 1",
          "/titleraw @s[hasitem={item=coldsink:machine_hero_weapon,location=slot.weapon.mainhand}] actionbar {\"rawtext\":[{\"text\":\"§f< §b\"},{\"score\":{\"name\":\"@s\",\"objective\":\"bullet_hero\"}},{\"text\":\"§f / §b120\"},{\"text\":\"§f >\"}]}"
        ]
      }
    },
    "animation.machine_hero_weapon_fire.func": {
      "loop": false,
      "animation_length": 10.0,
      "timeline": {
        "0.055": [
          "/event entity @s action_clear"
        ]
      }
    },
    "animation.machine_hero_attack_light.func": {
      "loop": false,
      "animation_length": 10.0,
      "timeline": {
        "0.0": [
          "/scoreboard players random @s wp_random 1 3",
          "/tag @s add self",
          "/execute @s[scores={wp_random=1}] ~~~ playsound machine_hero_attack @a[tag=!self] ~~~ 3 1 3",
          "/execute @s[scores={wp_random=2}] ~~~ playsound machine_hero_attack @a[tag=!self] ~~~ 3 0.8 3",
          "/execute @s[scores={wp_random=3}] ~~~ playsound machine_hero_attack @a[tag=!self] ~~~ 3 1.2 3",
          "/execute @s[scores={wp_random=1}] ~~~ playsound machine_hero_attack.self @s ~~~ 3 1 3",
          "/execute @s[scores={wp_random=2}] ~~~ playsound machine_hero_attack.self @s ~~~ 3 0.8 3",
          "/execute @s[scores={wp_random=3}] ~~~ playsound machine_hero_attack.self @s ~~~ 3 1.2 3",
          "/tag @s remove self"
        ],
        "0.2": [
          "/tag @s add machine_hero_self",

          "/execute @s[tag=maper] ^^1.75^ execute @e[family=monster,family=!fort,family=!tool,family=!tools,type=!item,tag=!neutral,r=4] ~~~ playsound get_target @p[tag=machine_hero_self,tag=!no_hit_sound]",
          "/execute @s[tag=maper] ^^1.75^ tag @e[family=monster,family=!fort,family=!tool,family=!tools,type=!item,tag=!neutral,r=4] add melee_damage",
          "/execute @s[tag=maper] ^^1.75^ damage @e[family=monster,family=!fort,family=!tool,family=!tools,type=!item,tag=!neutral,tag=!no_any_hurt,r=4] 20 suicide",

          "/execute @s[tag=!maper] ^^1.75^ execute @e[tag=!machine_hero_self,family=!fort,family=!tool,family=!tools,type=!item,r=4] ~~~ playsound get_target @p[tag=machine_hero_self,tag=!no_hit_sound]",
          "/execute @s[tag=!maper] ^^1.75^ damage @e[tag=!machine_hero_self,family=!fort,family=!tool,family=!tools,type=!item,tag=!no_any_hurt,r=4] 20 suicide",
          
          "/tag @s remove machine_hero_self"
        ],
        "0.4": [
          "/event entity @s action_clear"
        ]
      }
    },
    "animation.machine_hero_attack_heavy.func": {
      "loop": false,
      "animation_length": 10.0,
      "timeline": {
        "1.2": [
          "/tag @s add machine_hero_self",

          "/playsound machine_hero_attack_big @a[tag=!machine_hero_self] ~~~",
          "/playsound machine_hero_attack_big.self @s",

          "/execute @s[tag=maper] ^^3.25^ execute @e[family=monster,family=!fort,family=!tool,family=!tools,type=!item,tag=!neutral,r=5] ~~~ playsound get_target @p[tag=machine_hero_self,tag=!no_hit_sound]",
          "/execute @s[tag=maper] ^^3.25^ tag @e[family=monster,family=!fort,family=!tool,family=!tools,type=!item,tag=!neutral,r=5] add melee_damage",
          "/execute @s[tag=maper] ^^3.25^ damage @e[family=monster,family=!fort,family=!tool,family=!tools,type=!item,tag=!neutral,tag=!no_any_hurt,r=5] 38 suicide",

          "/execute @s[tag=!maper] ^^3.25^ execute @e[tag=!machine_hero_self,family=!fort,family=!tool,family=!tools,type=!item,r=5] ~~~ playsound get_target @p[tag=machine_hero_self,tag=!no_hit_sound]",
          "/execute @s[tag=!maper] ^^3.25^ damage @e[tag=!machine_hero_self,family=!fort,family=!tool,family=!tools,type=!item,tag=!no_any_hurt,r=5] 38 suicide",
          
          "/tag @s remove machine_hero_self"
        ],
        "1.5": [
          "/event entity @s action_clear"
        ]
      }
    },
    "animation.machine_hero_begin.func": {
      "loop": false,
      "animation_length": 10.0,
      "timeline": {
        "0.5": [
          "/replaceitem entity @s slot.hotbar 7 coldsink:machine_hero_skill_1 1 0 {\"item_lock\":{\"mode\":\"lock_in_slot\"}}",
          "/replaceitem entity @s slot.hotbar 8 coldsink:machine_hero_skill_2 1 0 {\"item_lock\":{\"mode\":\"lock_in_slot\"}}",
          "/event entity @s action_clear"
        ]
      }
    },
    "animation.machine_hero_skill_timer.func": {
      "loop": false,
      "animation_length": 10.0,
      "timeline": {
        "1.0": [
          "/event entity @s coldsink:machine_hero_skill"
        ],
        "1.3": [
          "/event entity @s action_clear"
        ]
      }
    }
  }
}
