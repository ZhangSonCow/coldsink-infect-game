{
  "format_version": "1.20.30",
  "minecraft:entity": {
    "description": {
      "identifier": "nacal_tac:raid_throw",
      "is_spawnable": false,
      "is_summonable": true,
      "is_experimental": false,
      "runtime_identifier": "minecraft:snowball"
    },
    "component_groups": {
      "raid_die": {
        "minecraft:instant_despawn": {}
      }
    },
    "components": {
      "minecraft:type_family": {
        "family": [ "bullet", "tool" ]
      },
      "minecraft:collision_box": {
        "width": 0.3,
        "height": 0.3
      },
      "minecraft:projectile": {
        "on_hit": {
          "definition_event": {
            "affect_projectile": true,
            "affect_shooter": false,
            "affect_target": false,
            "affect_splash_area": false,
            "event_trigger": {
              "event": "begin"
            }
          }
        },
        "power": 1.4,
        "gravity": 0.05,
        "angle_offset": -10
      },
      "minecraft:physics": {},
      "minecraft:conditional_bandwidth_optimization": {
        "default_values": {
          "max_optimized_distance": 80.0,
          "max_dropped_ticks": 10,
          "use_motion_prediction_hints": true
        }
      }
    },
    "events": {
      "minecraft:entity_spawned": {
        "queue_command": {
          "command": [
            "scoreboard players operation @s raid_time = raid_count raid_time",
            "tag @s add neutral"
          ]
        }
      },
      "begin": {
        "queue_command": {
          "command": [
            "playsound tac.raid @a[r=30] ~~~ 1 1 0.8",
            "particle nacal_tac:shellraid_circle ~ ~1 ~",
            "particle nacal_tac:raid_circle ~ ~1 ~",
            "summon nacal_tac:raid_z ~ ~ ~",
            "scoreboard players operation @e[type=nacal_tac:raid_z,c=1] raid_time = @s raid_time",
            "title @a[r=30] actionbar §l§c空袭接近中",
            "event entity @s godie"
          ]
        }
      },
      "godie": {
        "add": {
          "component_groups": [
            "raid_die"
          ]
        }
      }
    }
  }
}