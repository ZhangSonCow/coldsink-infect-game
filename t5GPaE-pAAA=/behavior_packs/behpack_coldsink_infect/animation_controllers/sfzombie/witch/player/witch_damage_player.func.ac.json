{
	"format_version": "1.12.0",
	"animation_controllers": {
		"controller.animation.witch_damage_player.func": {
			"initial_state": "damage_1",
			"states": {
				"health_down": {
					"transitions": [
						{
							"damage_2": "q.health <= 64 && q.health > 48"
						},
						{
							"damage_3": "q.health <= 48 && q.health > 32"
						},
						{
							"damage_4": "q.health <= 32 && q.health > 16"
						},
						{
							"damage_5": "q.health <= 16"
						}
					]
				},
				"damage_1": {
					"transitions": [
						{
							"health_down": "q.health <= 64"
						}
					],
					"on_entry": [
						"/scoreboard players set @s witch_damage 1",
						"/title @s actionbar §d当前攻击等级 §f1/5"
					]
				},
				"damage_2": {
					"transitions": [
						{
							"health_down": "q.health <= 48"
						},
						{
							"damage_1": "q.health > 64"
						}
					],
					"on_entry": [
						"/scoreboard players set @s witch_damage 2",
						"/title @s actionbar §d当前攻击等级 §f2/5"
					]
				},
				"damage_3": {
					"transitions": [
						{
							"health_down": "q.health <= 32"
						},
						{
							"damage_1": "q.health > 64"
						},
						{
							"damage_2": "q.health > 48"
						}
					],
					"on_entry": [
						"/scoreboard players set @s witch_damage 3",
						"/title @s actionbar §d当前攻击等级 §f3/5"
					]
				},
				"damage_4": {
					"transitions": [
						{
							"health_down": "q.health <= 16"
						},
						{
							"damage_1": "q.health > 64"
						},
						{
							"damage_2": "q.health > 48"
						},
						{
							"damage_3": "q.health > 32"
						}
					],
					"on_entry": [
						"/scoreboard players set @s witch_damage 4",
						"/title @s actionbar §d当前攻击等级 §f4/5"
					]
				},
				"damage_5": {
					"transitions": [
						{
							"damage_1": "q.health > 64"
						},
						{
							"damage_2": "q.health > 48"
						},
						{
							"damage_3": "q.health > 32"
						},
						{
							"damage_4": "q.health > 16"
						}
					],
					"on_entry": [
						"/scoreboard players set @s witch_damage 5",
						"/title @s actionbar §d当前攻击等级 §f5/5"
					]
				}
			}
		}
	}
}
