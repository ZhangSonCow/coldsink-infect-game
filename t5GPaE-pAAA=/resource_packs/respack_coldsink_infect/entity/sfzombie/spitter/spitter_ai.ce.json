{
  "format_version": "1.12.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "coldsink:spitter_ai",
      "animations": {
        "spitter_idle.arm": "animation.spitter.idle.arm",
        "spitter_idle.leg": "animation.spitter.idle.leg",
        "spitter_move.arm": "animation.spitter.move.arm",
        "spitter_move.leg": "animation.spitter.move.leg",
        "spitter_attack1": "animation.spitter.attack1",
        "spitter_attack2": "animation.spitter.attack2",
        "spitter_skill": "animation.spitter.skill",
        "dip": "animation.spitter.dip",
        "c.spitter_ai": "controller.animation.spitter_ai",
        "c.spitter_walk": "controller.animation.spitter_walk"
