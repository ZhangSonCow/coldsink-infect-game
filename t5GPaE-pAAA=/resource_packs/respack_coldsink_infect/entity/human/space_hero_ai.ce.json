{
  "format_version": "1.12.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "coldsink:space_hero_ai",
      "animations": {
        "space_hero_idle.arm": "animation.space_hero.idle.arm",
        "space_hero_idle.leg": "animation.space_hero.idle.leg",
        "space_hero_move.leg": "animation.space_hero.move.leg",
        "space_hero_attack": "animation.space_hero.attack",
        "c.space_hero_ai": "controller.animation.space_hero_ai",
        "c.space_hero_walk": "controller.animation.space_hero_walk",
        "space_hero.skill": "animation.space_hero.skill",
        "space_hero.particle": "animation.space_hero.particle",
        "space_hero.look": "animation.space_hero.look",
        "sign_facing": "animation.sign_facing"
      },
      "scripts": {
        "initialize": [
          "variable.player_x_rotation=0.0;"
        ],
        "pre_animation": [
          "variable.random_choices = math.random(0, 100);"
        ],
        "animate": [
          "c.space_hero_ai",
          "c.space_hero_walk",
          {
            "space_hero.particle": "!q.is_sheared"
          },
          "space_hero.look",
          "sign_facing"
        ]
      },
      "materials": {
        "default": "entity_alphatest",
        "nacal_tac_detect": "nacal_tac_detect",
        "nacal_customblend":"nacal_customblend"
      },
      "particle_effects": {
        "space_hero.particle": "coldsink:space_hero",
        "space_hero_attack": "nacal_sdc:space_hero_attack"
      },
      "textures": {
        "default": "textures/entity/space_hero",
        "space_hero_sign": "textures/entity/space_hero_sign"
      },
      "geometry": {
        "default": "geometry.space_hero",
        "human_sign": "geometry.human_sign"
      },
      "render_controllers": [ "controller.render.com", "controller.render.space_hero_sign" ],
      "spawn_egg": {
        "texture": "become_space_hero"
      }
    }
  }
}