{
  "format_version": "1.12.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "coldsink:human",
      "materials": {
        "default": "entity_alphatest",
        "nacal_tac_detect": "nacal_tac_detect",
        "nacal_customblend":"nacal_customblend",
        "netease_entity_metallic": "entity_alphatest"
      },
      "animations": {
        "jockey_control": "animation.jockey.control",
        "bob": "animation.player.bob"
      },
      "scripts": {
        "initialize": [
          "variable.is_holding_right = 0.0;",
          "variable.is_blinking = 0.0;",
          "variable.last_blink_time = 0.0;",
          "variable.hand_bob = 0.0;",
          "variable.player_x_rotation=0.0;"
        ],
        "pre_animation": [
          "variable.hand_bob = q.life_time < 0.01 ? 0.0 : variable.hand_bob + ((q.is_on_ground && q.is_alive ? math.clamp(math.sqrt(math.pow(q.position_delta(0), 2.0) + math.pow(q.position_delta(2), 2.0)), 0.0, 0.1) : 0.0) - variable.hand_bob) * 0.02;"
        ],
        "animate": [
          "bob",
          {
            "jockey_control": "q.out_of_control"
          }
        ]
      },
      "textures": {
        "default": "textures/entity/steve",
        "bile_sign": "textures/entity/bile_sign"
      },
      "geometry": {
        "default": "geometry.humanoid.custom",
        "bile_sign": "geometry.bile_sign"
      },
      "render_controllers": [
        "controller.render.com",
        {
          "controller.render.bile_sign": "q.is_stunned"
        }
      ],
      "spawn_egg": {
        "texture": "human_icon"
      }
    }
  }
}